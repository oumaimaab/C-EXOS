<Window x:Class="Exo1.explander"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Exo1"
        mc:Ignorable="d"
        Title="explander" Height="450" Width="800">
    <Grid>
        <Expander Header="Options" Margin="0,0,0.333,-0.333">
            <StackPanel Margin="26,0,-25.667,0" Height="380">
                <RadioButton Content=".NET" Margin="0,0,0.333,0"/>
                <RadioButton Content="SPRING CORE"/>
                <RadioButton Content="NODE JS"/>
            </StackPanel>



        </Expander>
    </Grid>
</Window>
